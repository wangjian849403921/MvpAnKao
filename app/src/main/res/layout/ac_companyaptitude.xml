<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f8f8f8"
    android:orientation="vertical">

    <include layout="@layout/common_header"/>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff"
                android:divider="@drawable/line"
                android:orientation="vertical"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:showDividers="middle">


                <TextView
                    android:id="@+id/AptitudeQuery"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/center_TextHeight"
                    android:background="#fff"
                    android:drawableLeft="@drawable/shape"
                    android:drawablePadding="10dp"
                    android:drawableRight="@drawable/icon_next_step"
                    android:gravity="center_vertical"
                    android:text="资质查询"
                    android:textColor="@color/centerTextColor"
                    android:textSize="@dimen/center_size"
                    />

                <TextView
                    android:id="@+id/GIL"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/center_TextHeight"
                    android:background="#fff"
                    android:drawableLeft="@drawable/shape"
                    android:drawablePadding="10dp"
                    android:drawableRight="@drawable/icon_next_step"
                    android:gravity="center_vertical"
                    android:text="GIL"
                    android:textColor="@color/centerTextColor"
                    android:textSize="@dimen/center_size"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:background="#fff"

                    android:drawablePadding="10dp"

                    android:gravity="center_vertical"
                    android:text=""
                    android:textColor="@color/centerTextColor"
                    android:textSize="@dimen/center_size"
                    />
            </LinearLayout>



</LinearLayout>
